# beep registry file

[aac]
longname = AAC LC decoder
description = Decode AAC LC compressed audio to raw data
mime= audio/mpeg, mpegversion=(int){2,4}, profile=(string)lc
dsp_lib = /usr/lib/imx-mm/audio-codec/wrap/lib_dsp_wrap_arm_elinux.so
library2 = /usr/lib/imx-mm/audio-codec/wrap/lib_aacplusd_wrap_arm_elinux.so
library = /usr/lib/imx-mm/audio-codec/wrap/lib_aacd_wrap_arm_elinux.so.3

[mp3]
longname = MP3 decoder
description = Decode MP3 compressed audio to raw data
dsp_lib = /usr/lib/imx-mm/audio-codec/wrap/lib_dsp_wrap_arm_elinux.so
mime = audio/mpeg, mpegversion = (int)1
library = /usr/lib/imx-mm/audio-codec/wrap/lib_mp3d_wrap_arm_elinux.so.3

[wma]
longname = WMA decoder
description = Decode WMA compressed audio to raw data
mime = audio/x-wma, wmaversion = (int)[ 1, 4 ]
dsp_lib = /usr/lib/imx-mm/audio-codec/wrap/lib_dsp_wrap_arm_elinux.so
library = /usr/lib/imx-mm/audio-codec/wrap/lib_wma10d_wrap_arm_elinux.so

[ac3]
longname = AC3 decoder
description = Decode AC3 compressed audio to raw data
dsp_lib = /usr/lib/imx-mm/audio-codec/wrap/lib_dsp_wrap_arm_elinux.so
mime = audio/x-ac3; audio/x-3ca
library = /usr/lib/imx-mm/audio-codec/wrap/lib_ac3d_wrap_arm_elinux.so

[vorbis]
longname = Vorbis decoder
description = Decode Vorbis compressed audio to raw data
rank = 259
mime = audio/x-vorbis
dsp_lib = /usr/lib/imx-mm/audio-codec/wrap/lib_dsp_wrap_arm_elinux.so
library = /usr/lib/imx-mm/audio-codec/wrap/lib_vorbisd_wrap_arm_elinux.so.3

[ddplus]
longname = DDplus decoder
description = Decode Dobly Digital Plus audio to raw data
mime = audio/x-eac3
dsp_lib = /usr/lib/imx-mm/audio-codec/wrap/lib_dsp_wrap_arm_elinux.so
library = /usr/lib/imx-mm/audio-codec/wrap/lib_ddpd_wrap_arm_elinux.so

[amr-nb]
longname = AMR nb decoder
description = decode amr nb audio to raw data
mime = audio/AMR;
dsp_lib = /usr/lib/imx-mm/audio-codec/wrap/lib_dsp_wrap_arm_elinux.so
library = /usr/lib/imx-mm/audio-codec/wrap/lib_nbamrd_wrap_arm_elinux.so.1

[amr-wb]
longname = AMR wb decoder
description = decode amr wb audio to raw data
mime = audio/AMR-WB
dsp_lib = /usr/lib/imx-mm/audio-codec/wrap/lib_dsp_wrap_arm_elinux.so
library = /usr/lib/imx-mm/audio-codec/wrap/lib_wbamrd_wrap_arm_elinux.so.1

[realaudio]
longname = Real Audio decoder
description = decode real audio cook to raw data
mime = audio/x-pn-realaudio, channels = (int)[ 1, 2 ]
library = /usr/lib/imx-mm/audio-codec/wrap/lib_realad_wrap_arm_elinux.so

