#!/bin/sh
[ "$1" != "upgrade" ] || exit 0
set -e
	update-alternatives --remove  i2cdetect /usr/sbin/i2cdetect.i2c-tools
	update-alternatives --remove  i2cdump /usr/sbin/i2cdump.i2c-tools
	update-alternatives --remove  i2cget /usr/sbin/i2cget.i2c-tools
	update-alternatives --remove  i2cset /usr/sbin/i2cset.i2c-tools
	update-alternatives --remove  i2ctransfer /usr/sbin/i2ctransfer.i2c-tools
